<div class='row'>
<h1>Top 10 Matches by Member</h1>
  <div class="col-md-12">
    <%= link_to 'Add Member', new_member_path, class: 'btn btn-primary flt-rg m-btm-10' %>
  </div>
  <div class='col-md-12'>
  <table class="table table-striped table-bordered table-responsive member-listing">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Average</th>
      </tr>
    </thead>
    <tbody>
      <% @top_10&.each do |member| %>
        <tr>
          <td><%= member[:name] %></td>
          <td><%= member[:average].round(2) %></td>
        </tr>
      <% end %> 
    </tbody>
  </table>
</div>
</div>